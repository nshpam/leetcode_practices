class Solution:
    def productExceptSelf(self, nums: List[int]) -> List[int]:
        """
        [1,2,3,4]

        another way is to store the prefix and post fix into an array
        """

        output = [1] * len(nums)
        temp = 1

        # prefix
        for i in range(len(nums)):
            output[i] = temp
            temp *= nums[i]

        temp = 1
        # postfix
        for j in range(len(nums)-1,-1,-1):
            output[j] *= temp
            temp *= nums[j]
