class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        """
        - count how many unique numbers are there
        - change nums array into an array with unique numbers
        """
        # [0,0,1,1,1,2,2,3,3,4]
        #                    ^j
        #          ^i
        # cost O(n)
        i=0 # the last index that is unique
        
        for j in range(1,len(nums)):
            # if the current number is the same as the check number then
            if nums[i] != nums[j]:
                i+=1
                nums[i] = nums[j]
        
        return i+1
